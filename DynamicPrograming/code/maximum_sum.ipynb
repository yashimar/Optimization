   "source": [
    "def max_sum(a):\n",
    "    N = len(a)\n",
    "    dp=[0]*(N+1)\n",
    "    for i in range(N):\n",
    "        dp[i+1]=max(dp[i],dp[i]+a[i])\n",
    "    return dp[N]"
   ]
  },
